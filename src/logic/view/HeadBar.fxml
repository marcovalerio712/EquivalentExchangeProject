<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<HBox spacing="10" fx:controller="logic.controller.graphic.HeaderBar" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" prefWidth="1280.0">
	<style>
		-fx-padding: 10;
	</style>
	<Button fx:id="homeButton" text="Home" onAction="#goToHome"/>
	<Button fx:id="catalogueButton" text="Catalogue" onAction="#goToCatalogue"/>
	<Button fx:id="communityButton" text="Community" onAction="#goToCommunity"/>
	<Pane HBox.hgrow="ALWAYS"/>
	<Button fx:id="loginButton" text="Login" onAction="#goToLogin"/>
	
</HBox>